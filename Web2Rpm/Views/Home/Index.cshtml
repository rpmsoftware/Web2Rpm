<div class="block">
	@model Web2Rpm.Models.Lead
	<div class="container">
		<div class="row">
			<div>
				<h2>@ViewData["Title"] </h2>
			</div>
			<div class="col-md-8 p-r-md">
				<hr style="margin-right:50px;" />
				@using (Html.BeginForm())
			{
					@Html.AntiForgeryToken()
					<div class="form-group row">
						<div class="col-md-12">
							@Html.LabelFor(m => m.Name, new { @class = "control-label" })
							@Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
							@Html.ValidationMessageFor(m => m.Name)
						</div>
					</div>
					<div class="form-group row">
						<div class="col-md-6 col-sm-6">
							@Html.LabelFor(m => m.Phone, new { @class = "control-label" })
							@Html.TextBoxFor(m => m.Phone, new { @class = "form-control" })
							@Html.ValidationMessageFor(m => m.Phone)
						</div>
						<div class="col-md-6 col-sm-6">
							@Html.LabelFor(m => m.Email, new { @class = "control-label" })
							@Html.TextBoxFor(m => m.Email, new { @class = "form-control" })
							@Html.ValidationMessageFor(m => m.Email)
						</div>
					</div>
					<hr />
					<div class="form-group row">
						<div class="col-md-6 col-sm-6">
							@Html.LabelFor(m => m.Company, new { @class = "control-label" })
							@Html.TextBoxFor(m => m.Company, new { @class = "form-control" })
							@Html.ValidationMessageFor(m => m.Company)
						</div>
						<div class="col-md-6 col-sm-6">
							@Html.LabelFor(m => m.Website, new { @class = "control-label" })
							@Html.TextBoxFor(m => m.Website, new { @class = "form-control" })
							@Html.ValidationMessageFor(m => m.Website)
						</div>
					</div>
					<hr />

					<div class="form-group row">
						<div class="col-md-12">
							@Html.LabelFor(m => m.Comment, new { @class = "control-label" })
							@Html.TextAreaFor(m => m.Comment, new { @class = "form-control", @rows = "4" })
							@Html.ValidationMessageFor(m => m.Comment)
						</div>
					</div>

					
					<div class="form-group row">
						<div class="col-md-12 text-center">
							<input type="submit" class="btn btn-primary contact-form-submit" value="Send" />
						</div>
					</div>
			}
			</div>
		</div>
	</div>
</div>